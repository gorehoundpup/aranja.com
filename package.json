  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "gsap": "^1.19.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.2",
    "react-chain": "^0.3.0",
    "react-chain-history": "^0.3.0",
    "react-document": "^0.5.0",
    "react-dom": "^15.5.2",
    "react-markdown": "^2.5.0",
    "react-visibility-sensor": "^3.9.0",
    "tux": "^0.2.1",
    "tux-adapter-contentful": "0.2.3",
    "universal-router": "^2.0.0",
    "window-or-global": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "node-sass-chokidar": "0.0.1",
    "npm-run-all": "^4.0.2",
    "tux-scripts": "^0.2.3"
  },
  "scripts": {
    "start-js": "tux-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && tux-scripts build",
    "serve": "tux-scripts serve",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "now-start": "tux-scripts serve"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
